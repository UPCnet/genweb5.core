  <!-- Para que se pueda selecionar la graella de portlets o subhome como vista por defecto carpeta -->
  <record name="plone.default_page_types" interface="plone.base.interfaces.controlpanel.ITypesSchema" field="default_page_types">
    <field type="plone.registry.field.List">
      <default>
        <element>Document</element>
        <element>Event</element>
        <element>News Item</element>
        <element>Collection</element>
      </default>
      <description xmlns:ns0="http://xml.zope.org/namespaces/i18n" ns0:domain="plone" ns0:translate="">The content types that should be available for selection when setting a default page.</description>
      <missing_value/>
      <required>False</required>
      <title xmlns:ns0="http://xml.zope.org/namespaces/i18n" ns0:domain="plone" ns0:translate="">Types that can be set as a default page</title>
      <value_type type="plone.registry.field.TextLine"/>
    </field>
    <value>
      <element>Document</element>
      <element>Event</element>
      <element>News Item</element>
      <element>Collection</element>
      <element>genweb.upc.subhome</element>
    </value>
  </record>

  <!-- Para que añada el idioma a la url -->
  <record name="plone.use_path_negotiation" interface="plone.app.multilingual.interfaces.IMultiLanguageExtraOptionsSchema" field="use_path_negotiation">
    <field type="plone.registry.field.Bool">
      <default>False</default>
      <description xmlns:ns0="http://xml.zope.org/namespaces/i18n" ns0:domain="plone" ns0:translate="description_language_codes_in_URL">Use language codes in URL path for manual override.</description>
      <required>False</required>
      <title xmlns:ns0="http://xml.zope.org/namespaces/i18n" ns0:domain="plone" ns0:translate="heading_language_codes_in_URL">Use language codes in URL path for manual override</title>
    </field>
    <value>True</value>
  </record>

  <record name="plone.use_cookie_negotiation" interface="plone.app.multilingual.interfaces.IMultiLanguageExtraOptionsSchema" field="use_cookie_negotiation">
    <field type="plone.registry.field.Bool">
      <default>False</default>
      <description xmlns:ns0="http://xml.zope.org/namespaces/i18n" ns0:domain="plone" ns0:translate="description_cookie_manual_override">Required for the language selector viewlet to be rendered.</description>
      <required>False</required>
      <title xmlns:ns0="http://xml.zope.org/namespaces/i18n" ns0:domain="plone" ns0:translate="heading_cookie_manual_override">Use cookie for manual override</title>
    </field>
    <value>True</value>
  </record>

  <!-- Añadir Lunes como primer día de la semana -->
  <record name="plone.first_weekday" interface="Products.CMFPlone.interfaces.controlpanel.IDateAndTimeSchema" field="first_weekday">
    <field type="plone.registry.field.Choice">
      <description xmlns:ns0="http://xml.zope.org/namespaces/i18n" ns0:domain="plone" ns0:translate="help_first_weekday">First day in the week.</description>
      <title xmlns:ns0="http://xml.zope.org/namespaces/i18n" ns0:domain="plone" ns0:translate="label_first_weekday">First weekday</title>
      <vocabulary>plone.app.vocabularies.Weekdays</vocabulary>
    </field>
    <value>0</value>
  </record>

  <!-- Syndication enabled by default -->
  <record name="Products.CMFPlone.interfaces.syndication.ISiteSyndicationSettings.default_enabled" interface="Products.CMFPlone.interfaces.syndication.ISiteSyndicationSettings" field="default_enabled">
    <field type="plone.registry.field.Bool">
      <default>False</default>
      <description>If syndication should be enabled by default for all folders and collections.</description>
      <title>Enabled by default</title>
    </field>
    <value>True</value>
  </record>
  <record name="genweb.hidden_settings.languages_applied" field="languages_applied">
    <field type="plone.registry.field.Bool">
      <default>False</default>
      <description>A field that let us know if the site has been set up</description>
      <title>languages_applied</title>
    </field>
    <value>False</value>
  </record>

  <!-- Tamaños de las miniaturas a mini -->
  <record name="plone.thumb_scale_listing" interface="plone.base.interfaces.controlpanel.ISiteSchema" field="thumb_scale_listing">
    <field type="plone.registry.field.Choice">
      <default>thumb</default>
      <description xmlns:ns0="http://xml.zope.org/namespaces/i18n" ns0:domain="plone" ns0:translate="">E.g. standard view; This default can be overriden individually.</description>
      <title xmlns:ns0="http://xml.zope.org/namespaces/i18n" ns0:domain="plone" ns0:translate="">Thumb scale for listings</title>
      <vocabulary>plone.app.vocabularies.ImagesScales</vocabulary>
    </field>
    <value>mini</value>
  </record>

  <record name="plone.thumb_scale_portlet" interface="plone.base.interfaces.controlpanel.ISiteSchema" field="thumb_scale_portlet">
    <field type="plone.registry.field.Choice">
      <default>icon</default>
      <description xmlns:ns0="http://xml.zope.org/namespaces/i18n" ns0:domain="plone" ns0:translate="">This default can be overriden individually.</description>
      <title xmlns:ns0="http://xml.zope.org/namespaces/i18n" ns0:domain="plone" ns0:translate="">Thumb scale for portlets</title>
      <vocabulary>plone.app.vocabularies.ImagesScales</vocabulary>
    </field>
    <value>mini</value>
  </record>

  <record name="plone.thumb_scale_summary" interface="plone.base.interfaces.controlpanel.ISiteSchema" field="thumb_scale_summary">
    <field type="plone.registry.field.Choice">
      <default>mini</default>
      <description xmlns:ns0="http://xml.zope.org/namespaces/i18n" ns0:domain="plone" ns0:translate="">This default can be overriden individually.</description>
      <title xmlns:ns0="http://xml.zope.org/namespaces/i18n" ns0:domain="plone" ns0:translate="">Thumb scale for summary view</title>
      <vocabulary>plone.app.vocabularies.ImagesScales</vocabulary>
    </field>
    <value>mini</value>
  </record>

  <record name="plone.thumb_scale_table" interface="plone.base.interfaces.controlpanel.ISiteSchema" field="thumb_scale_table">
    <field type="plone.registry.field.Choice">
      <default>tile</default>
      <description xmlns:ns0="http://xml.zope.org/namespaces/i18n" ns0:domain="plone" ns0:translate="">E.g., tabular view, folder content listing; This default can be overriden individually.</description>
      <title xmlns:ns0="http://xml.zope.org/namespaces/i18n" ns0:domain="plone" ns0:translate="">Thumb scale for tables</title>
      <vocabulary>plone.app.vocabularies.ImagesScales</vocabulary>
    </field>
    <value>mini</value>
  </record>

