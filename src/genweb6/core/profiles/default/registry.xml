  <!-- Mides editor tinymce -->
  <record name="plone.editor_height" interface="plone.base.interfaces.controlpanel.ITinyMCESchema" field="editor_height">
    <field type="plone.registry.field.TextLine">
      <description xmlns:ns0="http://xml.zope.org/namespaces/i18n" ns0:domain="plone" ns0:translate="">This option gives you the ability to specify the height of the editor in pixels. If auto resize is enabled this value is used as minimum height.</description>
