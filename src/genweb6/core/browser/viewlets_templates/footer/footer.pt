<div id="footer-complementary" i18n:domain="genweb"
     tal:attributes="data-class view/getClass;
                     data-img view/getHeroURL">

    <div class="row">

        <div class="col-md-4 text-primary">
            <p tal:content="structure view/getSignatura"></p>
        </div>

        <div class="col-md-8">
            <nav id="complimentary-footer-nav" aria-label="Complimenatary-footer">
                <ul id="footer-menu-complementari" class="footer-container"
                    tal:define="links view/getLinksPeu">

                    <li tal:define="link links/logo"
                        tal:condition="link">
                        <span class="text-muted me-2"
                              i18n:domain="genweb"
                              i18n:translate="">
                            Desenvolupat amb
                        </span>
                        <a tal:attributes="href link/url;
                                           target link/target">
                            <img title="genweb UPC" alt="genweb UPC" class="logoGenwebUPC"
                                 tal:attributes="src string:${view/root_url}/++theme++genweb6.theme/img/genwebUPC.png" />
                        </a>
                    </li>

                    <li tal:define="link links/contact"
                        tal:condition="link">
                        <a tal:attributes="href link/url;
                                           target link/target"
                           accesskey="9"
                           i18n:domain="genweb"
                           i18n:translate="">
                            Contacte amb nosaltres
                        </a>
                    </li>

                    <li tal:define="link links/sitemap"
                        tal:condition="link">
                        <a tal:attributes="href link/url;
                                           target link/target"
                           accesskey="3"
                           i18n:domain="plone"
                           i18n:translate="Site Map">
                            Site Map
                        </a>
                    </li>

                    <li tal:define="link links/accessibility"
                        tal:condition="link">
                        <a tal:attributes="href link/url;
                                           target link/target"
                           accesskey="0"
                           i18n:translate="genweb-accessibility">
                            Accessibilitat
                        </a>
                    </li>

                    <li tal:define="link links/disclaimer"
                        tal:condition="link">
                        <a tal:attributes="href link/url;
                                           target link/target"
                           i18n:translate="genweb-disclaimer">
                            Disclaimer
                        </a>
                    </li>

                    <li
                        tal:define="link links/cookies"
                        tal:condition="link">
                        <a tal:attributes="href link/url;
                                           target link/target;
                                           onClick link/onClick" i18n:translate="cookie_policy">
                            Cookie Policy
                        </a>
                    </li>
                </ul>
            </nav>
        </div>
    </div>
</div>
